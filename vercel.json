{
  "version": 2,
  "builds": [
    {
      "src": "backend/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/server.js"
    },
    {
      "src": "/webhook",
      "dest": "backend/server.js"
    },
    {
      "src": "/admin/(.*)",
      "dest": "admin-dashboard/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "$1"
    }
  ]
}
